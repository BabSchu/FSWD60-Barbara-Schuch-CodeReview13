<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Events" table="events">
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="eventName" type="string" column="event_name" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="eventDatetime" type="datetime" column="event_datetime" nullable="false"/>
    <field name="eventDescription" type="string" column="event_description" length="500" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="eventImage" type="string" column="event_image" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="eventCapacity" type="integer" column="event_capacity" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="eventContact" type="string" column="event_contact" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="eventlocationName" type="string" column="eventlocation_name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="eventlocationAddress" type="string" column="eventlocation_address" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="eventlocationZipCity" type="string" column="eventlocation_ZIP_city" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="eventUrl" type="string" column="event_url" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="eventType" type="string" column="event_type" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
